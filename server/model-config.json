{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "Email": {
    "dataSource": "email"
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "day": {
    "dataSource": "db",
    "public": false
  },
  "period": {
    "dataSource": "db",
    "public": false
  },
  "selection": {
    "dataSource": "db",
    "public": true,
    "options": { "remoting": { "sharedMethods": { "*": false } } }
  },
  "participation": {
    "dataSource": "db",
    "public": true,
    "options": { "remoting": { "sharedMethods": { "*": false } } }
  },
  "user": {
    "dataSource": "db",
    "public": true,
    "options": {
      "emailVerificationRequired": true,
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create": true,
          "deleteById": true,
          "login": true,
          "logout": true,
          "findById": true,
          "prototype.patchAttributes": true,
          "replaceById": true,
          "confirm": true,
          "resetPassword": true,
          "changePassword": true,
          "setPassword": true
        }
      }
    }
  },
  "event": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create": true,
          "findById": true,
          "deleteById": true,
          "replaceById": true,
          "prototype.patchAttributes": true,
          "findOwnedEvents": true,
          "claim": true,
          "participation_find": true,
          "participation_findById": true,
          "participation_create": true,
          "participation_selection_create": true,
          "participation_selection_deleteById": true
        }
      }
    }
  }
}
